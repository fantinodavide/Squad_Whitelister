{
  "name": "squad-whitelister",
  "version": "1.7.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "app": "vue-tsc --noEmit && vite build && node server",
    "releasezipold": "npm run build && powershell Compress-Archive dist release.zip -Force && powershell Compress-Archive server.js release.zip -Update && powershell Compress-Archive package-lock.json release.zip -Update && powershell Compress-Archive package.json release.zip -Update",
    "releasezip": "npm run build && npm run release && powershell Compress-Archive release release.zip -Force",
    "release": "npm run clean && npm run copy-dist && npm run minify-server && npm run copy-docker && npm run copy-certs && npm run filter-package && npm run generate-docs && npm run copy-docs",
    "releasedocker": "npm run release && docker build . && docker save squadwhitelister > docker-image.tar",
    "clean": "rimraf release",
    "copy-dist": "copyfiles -u 1 \"dist/**/*\" \"dist/**/.*\" release/dist",
    "minify-server": "terser --compress --mangle -o release/server.js -- server.js",
    "copy-docker": "copyfiles -f docker-compose.yml release",
    "copy-certs": "copyfiles -u 0 certificates/**/* release",
    "filter-package": "node packageInfoFilter.js",
    "generate-docs": "node utils/api-docs-generator.js",
    "copy-docs": "copyfiles -u 0 docs/api/docs.json release",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "bson": "^4.7.0",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-force-ssl": "^0.3.2",
    "express-rate-limit": "^8.2.1",
    "find-free-port": "^2.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "https": "^1.0.0",
    "husky": "^9.0.10",
    "jquery": "^3.6.0",
    "js-levenshtein": "^1.1.6",
    "log4js": "^6.4.4",
    "minimist": "^1.2.6",
    "mobile-detect": "^1.4.5",
    "mongo-sanitizer": "^1.0.4",
    "mongodb": "^4.5.0",
    "nocache": "^3.0.3",
    "node-run-cmd": "^1.0.1",
    "node-stream-zip": "^1.15.0",
    "node-zip": "^1.1.1",
    "path": "^0.12.7",
    "socket.io-client": "^4.5.1",
    "tar": "^7.0.0",
    "vue": "^3.2.31",
    "vue-marquee-text-component": "^2.0.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/js-levenshtein": "^1.1.1",
    "@types/node": "^16.11.39",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/tsconfig": "^0.1.3",
    "@wdns/vue-code-block": "^2.3.3",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.11",
    "rimraf": "^5.0.10",
    "swagger-autogen": "^2.23.7",
    "terser": "^5.39.2",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^1.6.5"
  }
}
